project 'UberEats.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'UberEats' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for UberEats
  pod 'Stripe'
  
  # Pods JSON
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'SwiftyJSON'
  
  # Pods for Firebase
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  
  # Pods for Facebook
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'

end
